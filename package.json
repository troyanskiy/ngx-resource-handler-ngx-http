    "@angular/core": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@types/node": "^8.0.34",
    "awesome-typescript-loader": "^3.2.3",
    "clean-webpack-plugin": "^0.1.17",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.3",
